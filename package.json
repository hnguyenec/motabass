{
  "name": "motabass",
  "version": "0.0.1",
  "author": "motabass",
  "description": "nx monorepo for my web stuff",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "deploy": "ng deploy",
    "lint": "nx workspace-lint && ng lint",
    "lint:precommit": "tslint --project . --config tslint.precommit.json --fix --force",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update:check": "ng update",
    "update:nx": "ng update @nrwl/workspace --allow-dirty --force",
    "update:cli": "ng update @angular/cli --allow-dirty",
    "update:angular": "ng update @angular/core --allow-dirty",
    "update:material": "ng update @angular/material --allow-dirty",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:precommit"
    ]
  },
