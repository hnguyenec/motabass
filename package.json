    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "@angular/service-worker": "9.1.0",
    "assert": "^2.0.0",
    "browser-id3-writer": "^4.4.0",
    "browserfs": "^1.4.3",
    "buffer": "^5.5.0",
    "core-js": "^3.6.4",
    "d3": "^5.15.1",
    "date-fns": "^2.11.1",
    "fast-memoize": "^2.5.2",
    "firebase": "^7.13.2",
