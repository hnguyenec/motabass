    "lucene-escape-query": "^1.0.1",
    "material-community-components": "^6.0.0",
    "mobx": "^5.15.4",
    "mobx-angular": "^4.1.0",
    "music-metadata-browser": "^2.0.5",
    "ngx-date-fns": "6.2.1",
    "ngx-electron": "^2.2.0",
    "ngx-indexed-db": "^4.0.14",
    "ngx-pipes": "2.7.3",
    "ngx-webstorage": "5.0.0",
    "node-vibrant": "^3.1.5",
    "process": "^0.11.10",
