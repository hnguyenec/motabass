{
  "name": "motabass",
  "version": "0.23.0",
  "author": "motabass",
  "description": "nx monorepo for my web stuff",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "npm run postinstall.web && npm run serve:gblaster",
    "start:electron": "run-p serve:gblaster serve:electron",
    "serve:gblaster": "nx serve gblaster",
    "build": "npm run postinstall.web && nx build gblaster",
    "build:prod": "nx build --prod",
    "build:prod:gblaster": "nx build gblaster --prod",
    "build:visuals": "nx build ui-components-visuals --prod",
    "publish:visuals": "npm run build:visuals && cd dist/libs/ui-components/visuals/ && npm publish --access public",
    "test": "npm run postinstall.web && ng test",
    "deploy": "ng deploy",
    "lint": "nx workspace-lint & ng lint --fix",
    "lint:styles": "stylelint \"{apps,libs}/**/*.{scss,html,ts}\" --fix",
    "lint:precommit": "tslint --project . --config tslint.precommit.json --fix --force",
    "e2e": "npm run postinstall.web && nx e2e",
    "e2e:watch": "npm run postinstall.web && nx e2e gblaster-e2e --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "prettier": "prettier --config ./.prettierrc --write \"{apps,libs,tools,.github}/**/*.{scss,html,ts,js,json,md,yml,yaml,css}\"",
    "upgrade": "ncu -ui",
    "update:check": "ng update",
    "update:nx": "ng update @nrwl/workspace --allow-dirty --force",
    "update:cli": "ng update @angular/cli --allow-dirty",
    "update:angular": "ng update @angular/core --allow-dirty",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "_postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && electron-rebuild install-app-deps",
    "generate:ui-lib": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=mtb --publishable --simpleModuleName",
    "generate:lazy-module-lib": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=hsr",
    "assets:optimize:svg": "svgo -f apps/gblaster/src/assets --disable=removeViewBox",
    "bundle-analyze:build:gblaster": "npm run build -- --stats-json && npm run bundle-analyze:gblaster",
    "bundle-analyze:gblaster": "webpack-bundle-analyzer dist/apps/gblaster/stats.json --port 8889",
    "bundle-analyze:static:gblaster": "webpack-bundle-analyzer dist/apps/gblaster/stats.json --mode static --report docs/analyzer-report.html --no-open",
    "bundle-analyze:build:static:gblaster": "npm run build -- --stats-json && webpack-bundle-analyzer dist/apps/gblaster/stats.json --mode static --report docs/analyzer-report.html --no-open",
    "release": "bump minor --tag --commit --push --no-verify -a && git checkout release && git merge master --no-verify && git push && git checkout master",
    "ci:build": "npm run build:prod:gblaster",
    "ci:e2e": "nx run gblaster-e2e:e2e --headless",
    "ci:deploy": "nx deploy gblaster",
    "compodoc:gblaster": "compodoc -p apps/gblaster/tsconfig.app.json",
    "compodoc:gblaster:serve": "npm run compodoc:gblaster -- -s",
    "destiny:gblaster": "destiny apps/gblaster/src/app/**/*.*",
    "destiny:visuals": "destiny libs/ui-components/visuals/src/lib/**/*.*",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron.gblaster": "npm run prepare.electron.gblaster && ng build electron-gblaster --prod --base-href ./",
    "build.electron.gblaster.local": "npm run build.electron.gblaster && electron dist/apps/electron-gblaster",
    "build.electron.gblaster.linux": "npm run build.electron.gblaster && cd dist/apps/electron-gblaster && npx electron-builder build --linux",
    "build.electron.gblaster.windows": "npm run build.electron.gblaster && cd dist/apps/electron-gblaster && npx electron-builder build --windows",
    "build.electron.gblaster.mac": "npm run build.electron.gblaster && cd dist/apps/electron-gblaster && npx electron-builder build --mac",
    "prepare.electron.gblaster": "npm run postinstall.electron && tsc -p apps/electron-gblaster/tsconfig.json",
    "serve.electron.gblaster.target": "nx serve electron-gblaster",
    "serve.electron.gblaster": "tsc -p apps/electron-gblaster/tsconfig.json && wait-on http-get://localhost:4200/ && electron apps/electron-gblaster/src --serve",
    "start.electron.gblaster": "npm-run-all -p serve.electron.gblaster.target serve.electron.gblaster",
    "generate:icon-set": "ts-node --project tools/tsconfig.tools.json tools/convertIcons.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:precommit"
    ]
  },
  "dependencies": {
    "@allex/crc32": "^1.0.4",
    "@angular/animations": "9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "9.1.0",
