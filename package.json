{
  "name": "motabass",
  "version": "0.1.0",
  "author": "motabass",
  "description": "nx monorepo for my web stuff",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "npm run serve:motabass",
    "start:electron": "run-p serve:motabass serve:electron",
    "serve:electron": "nx serve motabass-desktop",
    "build:electron": "nx build motabass-desktop",
    "build:electron:prod": "nx build motabass-desktop --prod",
    "package:electron": "nx run motabass-desktop:package",
    "make:electron": "nx run motabass-desktop:make",
    "serve:motabass": "nx serve motabass",
    "build": "nx build",
    "build:prod": "nx build --prod",
    "build:prod:motabass": "nx build motabass --prod",
    "build:visuals": "nx build ui-components-visuals --prod",
    "publish:visuals": "npm run build:visuals && cd dist/libs/ui-components/visuals/ && npm publish --access public",
    "test": "ng test",
    "deploy": "ng deploy",
    "lint": "nx workspace-lint && ng lint",
    "lint:styles": "stylelint \"{apps,libs}/**/*.{scss,html,ts}\"",
    "lint:precommit": "tslint --project . --config tslint.precommit.json --fix --force",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "prettier": "prettier --config ./.prettierrc --write \"{apps,libs}/**/*.{scss,html,ts}\"",
    "update:check": "ng update",
    "update:nx": "ng update @nrwl/workspace --allow-dirty --force",
    "update:cli": "ng update @angular/cli --allow-dirty",
    "update:angular": "ng update @angular/core --allow-dirty",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "generate:ui-lib": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=mtb --publishable --simpleModuleName",
    "generate:lazy-module-lib": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=hsr",
    "assets:optimize:svg": "svgo -f apps/motabass/src/assets --disable=removeViewBox",
    "bundle-analyze:build:motabass": "npm run build:prod:motabass -- --stats-json && npm run bundle-analyze:motabass",
    "bundle-analyze:motabass": "webpack-bundle-analyzer dist/apps/motabass/stats.json --port 8889",
    "compodoc:motabass": "compodoc -p apps/motabass/tsconfig.app.json",
    "compodoc:motabass:serve": "npm run compodoc:motabass -- -s",
    "destiny:motabass": "destiny apps/motabass/src/app/**/*.*",
    "destiny:visuals": "destiny libs/ui-components/visuals/src/lib/**/*.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:precommit"
    ]
  },
  "dependencies": {
    "@angular/animations": "9.0.4",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "9.0.4",
    "@angular/compiler": "9.0.4",
    "@angular/core": "9.0.4",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.0.4",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "9.0.4",
    "@angular/platform-browser-dynamic": "9.0.4",
    "@angular/router": "9.0.4",
    "@angular/service-worker": "9.0.4",
    "@nrwl/angular": "9.0.4",
    "angular2-hotkeys": "2.1.5",
    "browser-id3-writer": "^4.4.0",
    "browserfs": "^1.4.3",
    "core-js": "^3.6.4",
    "date-fns": "^2.10.0",
    "devtron": "^1.4.0",
    "electron-debug": "^3.0.1",
    "fast-memoize": "^2.5.1",
    "firebase": "^7.9.2",
    "jsmediatags": "^3.9.3",
    "material-community-components": "^6.0.0",
    "mobx": "^5.15.4",
    "mobx-angular": "^4.1.0",
    "music-metadata-browser": "^2.0.4",
    "ngx-date-fns": "6.1.0",
    "ngx-electron": "^2.2.0",
    "ngx-pipes": "2.7.3",
    "ngx-webstorage": "5.0.0",
    "node-vibrant": "^3.1.5",
    "rxjs": "~6.5.4",
    "tinycolor2": "^1.4.1",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.4",
    "@angular-devkit/build-angular": "0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
    "@angular/cli": "9.0.4",
    "@angular/compiler-cli": "9.0.4",
    "@angular/language-service": "9.0.4",
    "@babel/core": "7.8.6",
    "@compodoc/compodoc": "^1.1.11",
    "@nrwl/cypress": "9.0.4",
    "@nrwl/jest": "9.0.4",
    "@nrwl/node": "9.0.4",
    "@nrwl/storybook": "9.0.4",
    "@nrwl/workspace": "9.0.4",
    "@storybook/addon-knobs": "5.3.14",
    "@storybook/angular": "5.3.14",
    "@storybook/react": "5.3.14",
    "@types/jest": "25.1.3",
    "@types/jsmediatags": "~3.9.1",
    "@types/node": "~13.7.7",
    "@types/tinycolor2": "^1.4.2",
    "babel-loader": "8.0.6",
    "codelyzer": "~5.2.1",
    "cypress": "^4.1.0",
    "destiny": "^0.3.1",
    "dotenv": "8.2.0",
    "electron": "^8.0.2",
    "electron-packager": "^14.2.1",
    "eslint": "6.8.0",
    "firebase-tools": "^7.14.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.2.3",
    "inquirer": "^7.0.4",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "jest": "25.1.0",
    "jest-preset-angular": "8.0.0",
    "lint-staged": "^10.0.8",
    "ng-packagr": "^9.0.2",
    "npm-check-updates": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "nx-electron": "~9.0.0-beta.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rxjs-tslint": "^0.1.7",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.14.2",
    "svgo": "^1.3.2",
    "ts-jest": "25.2.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.10.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
